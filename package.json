{
	"name": "bulten-case-study",
	"version": "0.1.0",
	"private": true,
	"homepage": ".",
	"dependencies": {
		"@types/react": "^17.0.3",
		"@types/react-dom": "^17.0.3",
		"core-js": "^3.10.1",
		"postcss": "^8.2.10",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.2.0"
	},
	"scripts": {
		"start": "webpack s --node-env development --hot",
		"build": "webpack --node-env production",
		"test": "jest",
		"lint": "tsc --noEmit && eslint src/**/*.ts{,x} -c .eslintrc.js --ignore-path .eslintignore",
		"format": "eslint --fix src/**/*.ts{,x} -c .eslintrc.js --ignore-path .eslintignore",
		"prettier-format": "prettier --config .prettierrc src/**/*.ts{,x} --write"
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/reactTests/__mocks__/fileMock.js",
			"\\.(css|less|sass|scss)$": "<rootDir>/src/assets/"
		},
		"testPathIgnorePatterns": [
			"<rootDir>/node_modules/",
			"<rootDir>/src/assets/"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.13.15",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-env": "^7.13.15",
		"@babel/preset-react": "^7.13.13",
		"@babel/preset-typescript": "^7.13.0",
		"@types/jest": "^26.0.22",
		"@types/webpack-env": "^1.16.0",
		"@typescript-eslint/eslint-plugin": "^4.18.0",
		"@typescript-eslint/parser": "^4.18.0",
		"autoprefixer": "^10.2.5",
		"babel-jest": "^26.6.3",
		"babel-loader": "^8.2.2",
		"babel-preset-minify": "^0.5.1",
		"clean-webpack-plugin": "^4.0.0-alpha.0",
		"css-loader": "^5.2.2",
		"css-minimizer-webpack-plugin": "^2.0.0",
		"eslint": "^7.24.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-config-prettier": "^8.1.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.3.1",
		"jest": "^26.6.3",
		"mini-css-extract-plugin": "^1.4.1",
		"node-sass": "^5.0.0",
		"postcss-loader": "^5.2.0",
		"postcss-preset-env": "^6.7.0",
		"prettier": "^2.2.1",
		"react-hot-loader": "^4.13.0",
		"sass": "^1.32.8",
		"sass-loader": "^11.0.1",
		"style-loader": "^2.0.0",
		"terser-webpack-plugin": "^5.1.1",
		"typescript": "^4.2.4",
		"webpack": "^5.33.2",
		"webpack-cli": "^4.6.0",
		"webpack-dev-server": "^3.11.2"
	}
}
